<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    app:startDestination="@+id/profileScreen">

    <fragment
        android:id="@+id/profileScreen"
        android:name="sadjadtalakoob.ir.mydagger.ui.main.profile.ProfileFragment"
        android:label="@string/fragment_profile"
        tools:layout="@layout/fragment_profile">
    </fragment>

    <fragment
        android:id="@+id/postsScreen"
        android:name="sadjadtalakoob.ir.mydagger.ui.main.posts.PostFragment"
        android:label="@string/fragment_posts"
        tools:layout="@layout/fragment_posts">
    </fragment>

    <fragment
        android:id="@+id/photosScreen"
        android:name="sadjadtalakoob.ir.mydagger.ui.main.photos.PhotosFragment"
        android:label="@string/fragment_photos"
        tools:layout="@layout/fragment_photos">
    </fragment>
</navigation>